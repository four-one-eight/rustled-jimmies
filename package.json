{
  "name": "rustled-jimmies",
  "version": "3.0.0",
  "description": "A comic featuring plump lil dudes with cute butts crated by Sam Pratt.",
  "main": "gulpfile.js",
  "scripts": {
    "build": "gulp build && hugo -e production && gulp post",
    "clean": "rm -rf public resources",
    "clean-node": "rm -rf node_modules",
    "development": "gulp build && hugo -e development && gulp post",
    "start": "gulp build && hugo -e development serve --disableFastRender"
  },
  "repository": {
    "type": "git",
    "url": "git+https://nibbink@github.com/nibbink/rustled-jimmies.git"
  },
  "author": "Sam Pratt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nibbink/rustled-jimmies/issues"
  },
  "homepage": "https://github.com/nibbink/rustled-jimmies#readme",
  "engines": {
    "node": "10.15.1"
  },
  "resolutions": {
    "natives": "1.1.3"
  },
  "devDependencies": {
    "gulp": "^4.0.0",
    "gulp-cache": "^1.1.2",
    "gulp-gm": "0.0.9",
    "gulp-htmlmin": "^5.0.1",
    "gulp-imagemin": "^6.0.0",
    "gulp-postcss": "^7.0.1",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.0.1",
    "imagemin-mozjpeg": "^8.0.0",
